{
  "name": "react_webpack",
  "version": "1.0.0",
  "description": "testing app for createing react app without create-react-app service",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --mode production",
    "dev": "rimraf build && cross-env NODE_ENV=development webpack --mode development",
    "dev-server": "rimraf build && cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --mode development",
    "eslint": "eslint .",
    "eslint-fix": "eslint --fix \"src/**/*.js\"",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\""
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Stepan Chaparyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stepanchaparyan/reactFromScratch/issues"
  },
  "homepage": "https://github.com/stepanchaparyan/reactFromScratch#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{json|css|scss|md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^3.2.0",
    "cssnano-webpack-plugin": "^1.0.3",
    "csv-loader": "^3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "1.18.2",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dashboard": "^3.3.1",
    "webpack-dev-server": "^3.8.1",
    "xml-loader": "^1.2.1"
  }
}
